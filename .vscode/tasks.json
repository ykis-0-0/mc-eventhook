{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "gradle",
			"id": "${workspaceFolderBasename}_jar",
			"script": "jar",
			"description": "Assembles a jar archive containing the main classes.",
			"group": "build",
			"project": "${workspaceFolderBasename}",
			"buildFile": "${workspaceFolder}/build.gradle",
			"rootProject": "${workspaceFolderBasename}",
			"projectFolder": "${workspaceFolder}",
			"workspaceFolder": "${workspaceFolder}",
			"args": "",
			"javaDebug": false,
			"problemMatcher": [
				"$gradle"
			],
			"label": "gradle: jar"
		},
		{
			"type": "gradle",
			"id": "${workspaceFolderBasename}_test",
			"script": "test",
			"description": "Runs the test suite.",
			"group": "verification",
			"project": "${workspaceFolderBasename}",
			"buildFile": "${workspaceFolder}/build.gradle",
			"rootProject": "${workspaceFolderBasename}",
			"projectFolder": "${workspaceFolder}",
			"workspaceFolder": "${workspaceFolder}",
			"args": "",
			"javaDebug": false,
			"problemMatcher": [
				"$gradle"
			],
			"label": "gradle: test"
		},
		{
			"type": "gradle",
			"id": "${workspaceFolderBasename}_clean",
			"script": "clean",
			"description": "Deletes the build directory.",
			"group": "build",
			"project": "${workspaceFolderBasename}",
			"buildFile": "${workspaceFolder}/build.gradle",
			"rootProject": "${workspaceFolderBasename}",
			"projectFolder": "${workspaceFolder}",
			"workspaceFolder": "${workspaceFolder}",
			"args": "",
			"javaDebug": false,
			"problemMatcher": [
				"$gradle"
			],
			"label": "gradle: clean"
		}
	]
}
